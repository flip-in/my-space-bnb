<%= simple_form_for [spaceship] do |f| %>
  <%= f.input :name %>
  <%= f.input :spaceship_class %>
  <%= f.input :manufacturer %>
  <%= f.input :location %>
  <%= f.input :description %>
  <%= f.input :passengers %>
  <%= f.input :speed %>
  <%= f.input :length %>
  <%= f.input :crew %>
  <%= f.input :price %>
  <%= f.submit %>
<% end %>
